{
    "presets":
    [
        [
            "@babel/preset-env",
            {
                "corejs": "3"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins":
    [
        [
            "@babel/plugin-proposal-decorators",
            {
                "decoratorsBeforeExport": true
            }
        ],
        "@babel/plugin-proposal-class-properties",
        "babel-plugin-lit-property-types-from-ts"
    ]
}
