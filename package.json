{
  "name": "vtb-webcomponents",
  "private": false,
  "version": "1.0.8-beta3",
  "description": "Visual Tour Builder - Web Components Library",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc --watch",
    "build:prod": "npm run clean && npm run format && npm run lint && npm run analyze && npm run build",
    "clean": "rm -rf dist/esm dist/cjs",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/vtb-webcomponents.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat vtb-webcomponents.bundled.js | gzip -9 | wc -c ; rm vtb-webcomponents.bundled.js",
    "dev": "cross-env webpack serve --hot --mode development"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Huub Segers - Bureau97",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "@types/pubnub": "^7.4.1",
    "dayjs": "^1.11.10",
    "lit": "^3.0.1",
    "lodash": "^4.17.21",
    "pubnub": "^7.5.0",
    "murmurhash": "^2.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.2",
    "@ckeditor/ckeditor5-autosave": "^40.0.0",
    "@ckeditor/ckeditor5-build-inline": "^40.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^39.2.0",
    "@ckeditor/ckeditor5-editor-inline": "^40.0.0",
    "@ckeditor/ckeditor5-inspector": "^4.1.0",
    "@ckeditor/ckeditor5-theme-lark": "^40.0.0",
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@open-wc/testing": "^3.2.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.4",
    "@types/google.maps": "^3.54.5",
    "@types/lodash": "^4.14.200",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@web/dev-server": "^0.3.6",
    "@web/dev-server-legacy": "^2.0.3",
    "@web/test-runner": "^0.17.2",
    "@web/test-runner-playwright": "^0.10.2",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-lit-property-types-from-ts": "^1.1.3",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.33.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "fast-glob": "^3.3.2",
    "lit-analyzer": "^2.0.1",
    "prettier": "^3.0.3",
    "ts-lit-plugin": "^2.0.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "overrides": {
    "minimatch": "5.1.2"
  },
  "customElements": "custom-elements.json",
  "browserslist": [
    "last 5 chrome version",
    "last 5 firefox version",
    "last 5 safari version"
  ]
}
